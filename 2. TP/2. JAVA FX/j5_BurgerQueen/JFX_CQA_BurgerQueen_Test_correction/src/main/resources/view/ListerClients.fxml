<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="500.0" maxWidth="600.0" minHeight="500.0" minWidth="600.0"
            prefHeight="500.0"
            prefWidth="600.0" stylesheets="@style/burger_queen.css"
            xmlns="http://javafx.com/javafx/8.0.111" fx:controller="controller.ListerClientsController2">
    <children>
        <fx:include fx:id="barreMenu" source="BarreMenu.fxml"/>
        <TableView fx:id="tvClients" layoutX="38.0" layoutY="100.0" prefHeight="300.0" prefWidth="500.0">
            <columns>
                <TableColumn fx:id="tcNom" prefWidth="120.0" text="Nom"/>
                <TableColumn fx:id="tcPrenom" prefWidth="120.0" text="Prénom"/>
                <TableColumn fx:id="tcCourriel" prefWidth="264.0" text="Courriel"/>
            </columns>
        </TableView>
        <Label layoutX="38.0" layoutY="52.0" prefHeight="25.0" prefWidth="94.0" text="Recherche :"/>
        <TextField fx:id="tfFiltreClient" layoutX="140.0" layoutY="52.0" prefHeight="25.0" prefWidth="200.0"/>
        <Button fx:id="btnCreer" layoutX="129.0" layoutY="436.0" mnemonicParsing="false" onAction="#creerClient"
                text="Créer"/>
        <Button fx:id="btnModifier" layoutX="240.0" layoutY="436.0" mnemonicParsing="false" onAction="#modifierClient"
                text="Modifier"/>
        <Button fx:id="btnSupprimer" layoutX="363.0" layoutY="436.0" mnemonicParsing="false" onAction="#supprimerClient"
                text="Supprimer"/>
    </children>

</AnchorPane>
